{
  "manifest_version": 3,
  "name": "ACU JS Snippet Toolkit",
  "description": "Copy JavaScript snippets for Storyline; simple team management.",
  "version": "1.6.0",
  "action": {
    "default_title": "ACU JS Snippets",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "clipboardWrite",
    "tabs"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}